@model frontendnet.Models.Categoria

<div class="card mt-3">
    <div class="card-body">
        <div class="mb-3 ps-3">
            <label class="form-label">ID de Categoría</label>
            <div class="form-text text-body">@Model.CategoriaId</div>
        </div>
        <div class="mb-3 ps-3">
            <label class="form-label">Nombre</label>
            <div class="form-text text-body">@Model.Nombre</div>
        </div>
    </div>
</div>
